{
  "allure": {
    "directory": "allure-results",
    "links": [
        "{link}",
        "https://jira.com/browse/{issue}",
        "https://testrail.com/browse/{tms}"
    ]
  },
  "specflow": {
    "stepArguments": {
      "convertToParameters": "true",
      "paramNameRegex": "",
      "paramValueRegex": ""
    },
    "grouping": {
      "suites": {
        "parentSuite": "^parentSuite:?(.+)",
        "suite": "^suite:?(.+)",
        "subSuite": "^subSuite:?(.+)"
      },
      "behaviors": {
        "epic": "^epic:?(.+)",
        "story": "^story:?(.+)"
      }
    },
    "labels": {
      "severity": "^(normal|blocker|critical|minor|trivial)"
    },
    "links": {
      "issue": "^issue:(\\d+)",
      "tms": "^tms:(\\d+)"
    }
  }
}
